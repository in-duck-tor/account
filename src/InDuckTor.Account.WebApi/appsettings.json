{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "AccountDatabase": ""
  },
  "DatabaseSettings": {
    "Scheme": "account"
  },
  "JwtSettings": {
