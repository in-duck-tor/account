        "BootstrapServers": ""
      },
      "Account": {
        "Topic": "in-duck-tor.account",
        "BootstrapServers": ""
      },
      "Transaction": {
        "Topic": "in-duck-tor.account.transaction",
        "BootstrapServers": ""
      }
    }
  }
}
